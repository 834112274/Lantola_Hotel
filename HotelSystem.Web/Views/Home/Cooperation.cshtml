<script>
    var cooperationanimateswitch = 1;
    $(document).ready(function () {
        $('.lantola_about_list li').click(function () {
            $('.lantola_about_list li').removeClass('hover');
            $(this).addClass('hover');
            $('.lantola_about_content').hide();
            $('.lantola_about_content').eq($(this).index()).show();
        })
        if ($('.cooperationanimate').length >= 1) {
            $(window).scroll(function () {
                if (cooperationanimateswitch == 1) {
                    $('.cooperationanimate1').css({ 'margin': '0', 'opacity': '1' });
                    setTimeout("$('.cooperationanimate2').css({'margin':'0','opacity':'1'})", 500);
                    setTimeout("$('.cooperationanimate3').css({'margin':'0','opacity':'1'})", 1000);
                }
                cooperationanimateswitch = 0;
            });
        }
        $('.personalpersonaldatadate2').click(function () {
            if ($(this).parent().find('.personalpersonaldatadate3 li').length == '0') { alert('请先填写年份和月份'); return false; }
            $(this).parent().find('.personalpersonaldatadate3').show();
            $('.mask').show();
        })
        $('.personalpersonaldatadate3 li').click(function () {
            $(this).parents('.personalpersonaldatadate1').find('.personalpersonaldatadate2').html($(this).html());
            $(this).parents('.personalpersonaldatadate1').find('input[type="hidden"]').val($(this).html());
            $(this).parents('.personalpersonaldatadate3').hide();
            $('.mask').hide();
        })
        $('.mask').click(function () {
            $('.personalpersonaldatadate3,.mask').hide();
        })
        $('.hotelregisterimageup2').change(function () {
            var objUrl = getObjectURL(this.files[0]);
            $(this).parent().css('background', 'none');
            /*$(this).parent().find('.hotelregisterimageup3').attr('src',objUrl);*/
            $(this).parent().find('.word').addClass('words');
            $(this).parent().find('.hotelregisterimageup4').css('background-image', 'url(' + objUrl + ')');
            $(this).parent().find('input[type="hidden"]').val(objUrl);
        })
        if (GetQueryString("article") != "") {
            if (GetQueryString("article") == "0") { $('.lantola_about_list li').eq(0).click(); }
            else if (GetQueryString("article") == "1") { $('.lantola_about_list li').eq(1).click(); }
            else if (GetQueryString("article") == "2") { $('.lantola_about_list li').eq(2).click(); }
            else if (GetQueryString("article") == "3") { $('.lantola_about_list li').eq(3).click(); }
            else if (GetQueryString("article") == "4") { $('.lantola_about_list li').eq(4).click(); }
            else if (GetQueryString("article") == "5") { $('.lantola_about_list li').eq(5).click(); }
            else if (GetQueryString("article") == "6") { $('.lantola_about_list li').eq(6).click(); }
            else if (GetQueryString("article") == "7") { $('.lantola_about_list li').eq(7).click(); }
        }//easeInExpo
        if ($('.cooperationbanner .banneranimate').length == 1) {
            $('.cooperationbanner .banneranimate1').animate({ 'width': '93.5px', 'height': '97.9px', 'margin': '-4.25px 0 0 -4.45px', 'opacity': '1' }, 750, "easeInExpo", function () {
                $(this).animate({ 'width': '85px', 'height': '89px', 'margin': '0', 'opacity': '1' }, 250, "easeInExpo")
            });
            $('.cooperationbanner .banneranimate2').animate({ 'width': '126.5px', 'height': '81.4px', 'margin': '-3.7px 0 0 -5.75px', 'opacity': '1' }, 750, "easeInExpo", function () {
                $(this).animate({ 'width': '115px', 'height': '74px', 'margin': '0', 'opacity': '1' }, 250, "easeInExpo")
            });
            $('.cooperationbanner .banneranimate1,.cooperationbanner .banneranimate2').css({ 'transform': 'rotateZ(360deg)' });
        }
    })

    function getObjectURL(file) {
        var url = null;
        if (window.createObjectURL != undefined) { // basic
            url = window.createObjectURL(file);
        } else if (window.URL != undefined) { // mozilla(firefox)
            url = window.URL.createObjectURL(file);
        } else if (window.webkitURL != undefined) { // webkit or chrome
            url = window.webkitURL.createObjectURL(file);
        }
        return url;
    }

    function GetQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]); return null;
    }
</script>

